<?php

/**
 * @file
 * Installation routines for DFS_DM feature
 */

/**
 * Implements hook_install().
 */
function dfs_dm_install() {
  // Revert df_assembler user permissions due to panelizer/features bug
  // http://drupal.org/node/1549608#comment-7312952
  features_get_features('df_assembler', TRUE);
  features_revert(array('df_assembler' => array('user_permission')));
}

