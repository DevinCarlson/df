{#
/**
 * @file
 * Template for large screen presentation (TV)
 *
 */
#}
<div {{ attributes.setAttribute('id', settings.css_id).addClass('row', 'align-center', 'lf-landscape-chrome', settings.css_classes) }}>
  <div class="large-11 lf-landscape columns">
    <div class="row align-middle">
      <div class="small-6 lf-left columns">
        {{ content.left }}
      </div>
      <div class="small-6 lf-right columns">
        {{ content.right }}
      </div>
    </div>
  </div>
</div>
<!-- /.lf-landscape -->
